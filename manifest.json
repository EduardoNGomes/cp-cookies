{
  "manifest_version": 3,
  "name": "CP Cookies",
  "version": "1.0",
  "description": "A simple extension to copy cookies and set cookies",
  "author": "Eduardo Gomes",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
	"tabs",
	"cookies",
	"storage"
  ],
  "host_permissions": [
	"http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}

